#: ui/livecd/finish/index.scm:7
msgid "All work is succesfully done."
msgstr "Todo o trabalho é feito com sucesso."

#: ui/livecd/start/index.scm:7
msgid "Attention:"
msgstr "Atenção:"

#: ui/livecd/start/index.scm:9
msgid "Close all open windows before you continue to work."
msgstr "Feche todas as janelas abertas antes de continuar a trabalhar."

#: standard input:5
msgid "Date/Time"
msgstr "Data/Hora"

#: ui/livecd/start/index.scm:11
msgid "During process all devices will be unmounted and all swap partitions will be turned off."
msgstr "Durante o processo de todos os equipamentos serão montados e todas as partições de swap será desligado."

#: ui/livecd/install/index.scm:24
msgid "Fatal error"
msgstr "Erro Fatal"

#: standard input:2
msgid "Finish installation"
msgstr "Instalação concluida"

#: ui/livecd/install/index.scm:38
msgid "Finishing..."
msgstr "Finalizando..."

#: standard input:4
msgid "Greeting"
msgstr "Postais"

#: ui/livecd/install/index.scm:39
msgid "Installation is done"
msgstr "Instalação é completa"

#: ui/livecd/install/index.scm:36
msgid "Installing system..."
msgstr "Instalação de sistema ..."

#: ui/livecd/finish/index.scm:9
msgid "New system is available at /mnt/destination mount point."
msgstr "Novo sistema está disponível em /mnt/destino ponto de montagem."

#: ui/livecd/install/index.scm:37
msgid "Preparing..."
msgstr "Preparado..."

#: standard input:6
msgid "System Administrator"
msgstr "Administrador do sistema"

#: standard input:1
msgid "System User"
msgstr "Usuário do Sistema"

#: standard input:3
msgid "System installation"
msgstr "Instalação do sistema"

#: backend3/livecd-start:32
msgid "Unable to create directory for new system"
msgstr "Não é possível criar o diretório para o novo sistema"

#: backend3/livecd-start:27
msgid "Unable to turn off swap partitions"
msgstr "Não é possível desligar as partições de swap"

#: backend3/livecd-start:20
msgid "Unable to unmount %s\\n"
msgstr "Não foi possível desmontar %s\\n"

